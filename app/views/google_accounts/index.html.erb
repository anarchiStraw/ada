<h1>利用できるGoogleアカウント</h1>
<table>
  <tr>
    <th>名前</th>
    <th>メールアドレス</th>
    <th></th>
  </tr>

<% @google_accounts.each do |google_account| %>
  <tr>
    <td><%= google_account.display_name %></td>
    <td><%= google_account.email %></td>
    <td><%= link_to '削除', google_account, confirm: '本当に?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= button_to '追加', verify_google_oauth_index_path %>

<%= button_to 'メニュー', menu_path %>
